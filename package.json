{
  "scripts": {
    "codegen:mainnet": "graph codegen subgraph.mainnet.yaml",
    "build:mainnet": "graph build subgraph.mainnet.yaml",
    "deploy:mainnet": "graph deploy --node https://api.thegraph.com/deploy/ alephao/Genzee subgraph.mainnet.yaml",
    "codegen:rinkeby": "graph codegen subgraph.rinkeby.yaml",
    "build:rinkeby": "graph build subgraph.rinkeby.yaml",
    "deploy:rinkeby": "graph deploy --node https://api.thegraph.com/deploy/ alephao/Genzee-Rinkeby subgraph.rinkeby.yaml",
    "create:local": "graph create --node http://localhost:8020/ alephao/Genzee",
    "remove:local": "graph remove --node http://localhost:8020/ alephao/Genzee",
    "deploy:local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 alephao/Genzee",
    "test": "graph test",
    "format": "prettier . -w",
    "prepare": "husky install"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.25.1",
    "@graphprotocol/graph-ts": "0.24.1"
  },
  "devDependencies": {
    "husky": "^7.0.4",
    "lint-staged": "^12.1.5",
    "matchstick-as": "^0.4.2",
    "prettier": "^2.5.1"
  },
  "lint-staged": {
    "*.-w": "prettier --write"
  }
}
